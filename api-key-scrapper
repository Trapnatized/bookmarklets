javascript:(function(){var results=new Set;var apiPatterns={'AWS Access Key':/AKIA[0-9A-Z]{16}/g,'AWS Secret Key':/([A-Za-z0-9+/]{40})/g,'GitHub Token':/gh[ps]_[0-9a-zA-Z]{36}/g,'Google API Key':/AIza[0-9A-Za-z\-_]{35}/g,'Firebase URL':/https?:\/\/[a-z0-9-]+\.firebaseio\.com/g,'Generic API Key':/['"`]((?:api|sk|pk)_[a-zA-Z0-9=_\-]{32,})['"`]/g,'JWT Token':/eyJ[a-zA-Z0-9_-]{10,}\.[a-zA-Z0-9_-]{10,}\.[a-zA-Z0-9_-]{10,}/g,'Basic Auth':/['"]Basic\s[a-zA-Z0-9=+\/_-]{5,}['"`]/g,'Bearer Token':/['"]Bearer\s[a-zA-Z0-9._\/_-]{5,}['"`]/g};function isValidAWSKey(key,type){if(type==='AWS Access Key'){return/^AKIA[0-9A-Z]{16}$/.test(key);}if(type==='AWS Secret Key'){return/^[A-Za-z0-9/+=]{40}$/.test(key)&&key.split('').some(char=>char>='a'&&char<='z')&&key.split('').some(char=>char>='A'&&char<='Z')&&key.split('').some(char=>char>='0'&&char<='9');}return true;}function findInScripts(){var scriptTags=document.getElementsByTagName('script');for(var i=0;i<scriptTags.length;i++){var script=scriptTags[i];var content=script.innerHTML;var src=script.src||'Inline Script';for(let[keyType,pattern]of Object.entries(apiPatterns)){var matches=content.matchAll(pattern);for(const match of matches){const value=match[1]||match[0];if(isValidAWSKey(value,keyType)){results.add({type:keyType,value:value,location:`Script: ${src}`,context:getContext(content,match.index),verified:keyType.includes('AWS')?'Matches AWS key format':'N/A'});}}}}}function findInAttributes(){var elements=document.getElementsByTagName('*');for(var i=0;i<elements.length;i++){var element=elements[i];for(var j=0;j<element.attributes.length;j++){var attr=element.attributes[j];for(let[keyType,pattern]of Object.entries(apiPatterns)){var matches=attr.value.matchAll(pattern);for(const match of matches){const value=match[1]||match[0];if(isValidAWSKey(value,keyType)){results.add({type:keyType,value:value,location:`Element: ${element.tagName}, Attribute: ${attr.name}`,context:getContext(attr.value,match.index),verified:keyType.includes('AWS')?'Matches AWS key format':'N/A'});}}}}}};function getContext(text,index){var start=Math.max(0,index-50);var end=Math.min(text.length,index+50);var context=text.substring(start,end);return`...${context}...`;};findInScripts();findInAttributes();const modal=document.createElement("div");modal.style.cssText="position:fixed;top:10%;left:50%;transform:translate(-50%,-10%);background:#1a1a1a;color:#fff;padding:20px;max-height:80vh;overflow-y:auto;z-index:99999;border-radius:8px;width:80%;font-family:monospace;box-shadow:0%200%2020px%20rgba(0,0,0,0.5);";const%20header=document.createElement("div");header.style.cssText="display:flex;justify-content:space-between;margin-bottom:20px;";const%20title=document.createElement("h3");title.textContent=`Found%20${results.size}%20potential%20secrets`;title.style.margin="0";title.style.color="#ff0000";const%20closeBtn=document.createElement("button");closeBtn.innerHTML="%C3%97";closeBtn.style.cssText="background:none;border:none;color:#ff4444;font-size:24px;cursor:pointer;padding:0%205px;";closeBtn.onclick=()=>modal.remove();header.appendChild(title);header.appendChild(closeBtn);modal.appendChild(header);const%20search=document.createElement("input");search.type="text";search.placeholder="Search...";search.style.cssText="width:100%;padding:8px;margin-bottom:10px;background:#333;border:1px%20solid%20#444;color:#fff;border-radius:4px;";modal.appendChild(search);const%20list=document.createElement("div");results.forEach(item=>{const%20entry=document.createElement("div");entry.style.cssText="padding:15px;border-bottom:1px%20solid%20#333;word-break:break-all;cursor:pointer;background:#222;margin-bottom:5px;border-radius:4px;";const%20typeSpan=document.createElement("span");typeSpan.textContent=`[${item.type}]%20`;typeSpan.style.color="#ff0000";const%20valueSpan=document.createElement("span");valueSpan.textContent=item.value;valueSpan.style.color="#fff";const%20locationSpan=document.createElement("div");locationSpan.textContent=`Location:%20${item.location}`;locationSpan.style.cssText="color:#888;font-size:12px;margin-top:5px;";const%20contextSpan=document.createElement("div");contextSpan.textContent=`Context:%20${item.context}`;contextSpan.style.cssText="color:#666;font-size:11px;margin-top:5px;background:#1a1a1a;padding:5px;border-radius:2px;";const%20verifiedSpan=document.createElement("div");verifiedSpan.textContent=`Verification:%20${item.verified}`;verifiedSpan.style.cssText=`color:${item.verified.includes('Matches')?'#4CAF50':'#FFA500'};font-size:12px;margin-top:5px;`;entry.appendChild(typeSpan);entry.appendChild(valueSpan);entry.appendChild(locationSpan);entry.appendChild(contextSpan);entry.appendChild(verifiedSpan);entry.onclick=()=>{navigator.clipboard.writeText(item.value);entry.style.backgroundColor="#2d2d2d";setTimeout(()=>entry.style.backgroundColor="#222",500);};list.appendChild(entry);});modal.appendChild(list);search.onkeyup=()=>{const%20term=search.value.toLowerCase();Array.from(list.children).forEach(entry=>{entry.style.display=entry.textContent.toLowerCase().includes(term)?"":"none";});};document.body.appendChild(modal);})();
