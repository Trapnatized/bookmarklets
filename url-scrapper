javascript:(function(){var scripts=document.getElementsByTagName("script"),regex=/(?<=(\"|\%27|\`))\/[a-zA-Z0-9_?&=\/\-\#\.]*(?=(\"|\'|\%60))/g,results=new%20Set,timeoutDelay=3000,endpointTypes={api:0,static:0,auth:0,other:0};function%20categorizeEndpoint(endpoint){if(endpoint.match(/\/(api|v1|v2|v3|graphql|rest)/i))return"api";if(endpoint.match(/\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot)$/i))return"static";if(endpoint.match(/\/(login|auth|oauth|signin|signup|register)/i))return"auth";return"other"}for(var%20i=0;i<scripts.length;i++){var%20src=scripts[i].src;if(src)fetch(src).then(response=>response.text()).then(content=>{let%20matches=content.matchAll(regex);for(let%20match%20of%20matches){results.add(match[0]);endpointTypes[categorizeEndpoint(match[0])]++}}).catch(error=>console.error("Error%20fetching%20script:",src,error))}var%20pageContent=document.documentElement.outerHTML,matches=pageContent.matchAll(regex);for(const%20match%20of%20matches){results.add(match[0]);endpointTypes[categorizeEndpoint(match[0])]++}function%20showResults(){const%20modal=document.createElement("div");modal.style.position="fixed",modal.style.top="10%",modal.style.left="50%",modal.style.transform="translate(-50%,%20-10%)",modal.style.background="#333",modal.style.color="#fff",modal.style.border="1px%20solid%20#444",modal.style.boxShadow="0%204px%208px%20rgba(0,%200,%200,%200.2)",modal.style.zIndex=9999,modal.style.padding="20px",modal.style.maxHeight="80%",modal.style.overflowY="auto",modal.style.fontFamily="Arial,%20sans-serif",modal.style.fontSize="14px",modal.style.borderRadius="8px",modal.style.width="50%",modal.style.textAlign="left",modal.style.direction="ltr";const%20header=document.createElement("div");header.style.display="flex",header.style.justifyContent="space-between",header.style.alignItems="center",header.style.marginBottom="10px";const%20title=document.createElement("h3");title.innerText=`Found%20${results.size}%20Endpoints`,title.style.margin="0",title.style.color="#ffa500";const%20stats=document.createElement("div");stats.style.marginTop="10px",stats.style.marginBottom="15px",stats.style.padding="10px",stats.style.background="#444",stats.style.borderRadius="4px",stats.style.fontSize="12px";stats.innerHTML=`%20%20%20%20%20<div%20style="display:flex;justify-content:space-between;margin-bottom:5px;">%20%20%20%20%20%20%20%20%20<span%20style="color:#4CAF50;">API%20Endpoints:%20${endpointTypes.api}</span>%20%20%20%20%20%20%20%20%20<span%20style="color:#2196F3;">Static%20Resources:%20${endpointTypes.static}</span>%20%20%20%20%20</div>%20%20%20%20%20<div%20style="display:flex;justify-content:space-between;">%20%20%20%20%20%20%20%20%20<span%20style="color:#FFC107;">Auth%20Endpoints:%20${endpointTypes.auth}</span>%20%20%20%20%20%20%20%20%20<span%20style="color:#9E9E9E;">Other:%20${endpointTypes.other}</span>%20%20%20%20%20</div>`;const%20closeButtonTop=document.createElement("span");closeButtonTop.innerText="%E2%9C%96",closeButtonTop.style.cursor="pointer",closeButtonTop.style.color="#fff",closeButtonTop.style.fontSize="18px",closeButtonTop.style.fontWeight="bold",closeButtonTop.style.marginLeft="10px",closeButtonTop.onclick=()=>document.body.removeChild(modal);header.appendChild(title),header.appendChild(closeButtonTop);const%20searchBar=document.createElement("input");searchBar.type="text",searchBar.placeholder="Search...",searchBar.style.width="100%",searchBar.style.padding="10px",searchBar.style.marginBottom="10px",searchBar.style.fontSize="14px",searchBar.style.border="1px%20solid%20#555",searchBar.style.borderRadius="4px",searchBar.style.outline="none",searchBar.style.boxSizing="border-box",searchBar.style.background="#222",searchBar.style.color="#fff";const%20list=document.createElement("ul");list.style.listStyleType="none",list.style.padding="0";results.forEach(item=>{const%20listItem=document.createElement("li");listItem.style.marginBottom="5px",listItem.style.wordBreak="break-word",listItem.style.textAlign="left",listItem.style.padding="5px",listItem.style.borderLeft="3px%20solid";const%20type=categorizeEndpoint(item);switch(type){case"api":listItem.style.borderColor="#4CAF50";break;case"static":listItem.style.borderColor="#2196F3";break;case"auth":listItem.style.borderColor="#FFC107";break;default:listItem.style.borderColor="#9E9E9E"}listItem.textContent=item,list.appendChild(listItem)});searchBar.addEventListener("input",()=>{const%20filter=searchBar.value.toLowerCase(),items=list.getElementsByTagName("li");for(let%20i=0;i<items.length;i++){const%20txtValue=items[i].textContent||items[i].innerText;items[i].style.display=txtValue.toLowerCase().indexOf(filter)>-1?"":"none"}});const%20downloadButton=document.createElement("button");downloadButton.innerText="Download%20Results",downloadButton.style.marginTop="10px",downloadButton.style.padding="5px%2010px",downloadButton.style.background="#4caf50",downloadButton.style.color="#fff",downloadButton.style.border="none",downloadButton.style.cursor="pointer",downloadButton.style.borderRadius="4px",downloadButton.onclick=()=>{const%20blob=new%20Blob([Array.from(results).join("\n")],{type:"text/plain"}),link=document.createElement("a");link.href=URL.createObjectURL(blob),link.download="extracted_paths.txt",link.click()};const%20closeButtonBottom=document.createElement("button");closeButtonBottom.innerText="Close",closeButtonBottom.style.marginTop="10px",closeButtonBottom.style.marginLeft="10px",closeButtonBottom.style.padding="5px%2010px",closeButtonBottom.style.background="#ffa500",closeButtonBottom.style.color="#000",closeButtonBottom.style.border="none",closeButtonBottom.style.cursor="pointer",closeButtonBottom.style.borderRadius="4px",closeButtonBottom.onclick=()=>document.body.removeChild(modal);modal.appendChild(header),modal.appendChild(stats),modal.appendChild(searchBar),modal.appendChild(list),modal.appendChild(downloadButton),modal.appendChild(closeButtonBottom),document.body.appendChild(modal)}setTimeout(showResults,timeoutDelay)})();
